    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/firebase/web"],
      "options": {
        "jestConfig": "libs/firebase/web/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
